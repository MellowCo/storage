  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.2",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.12",
